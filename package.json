{
    "name": "noise_cloud",
    "version": "1.0.0",
    "description": "Clone of Soundcloud",
    "main": "index.js",
    "author": "Sasha",
    "license": "ISC",
    "devDependencies": {
        "nodemon": "^2.0.4"
    },
    "dependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "@babel/node": "^7.10.1",
        "@babel/polyfill": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "autoprefixer": "^9.8.0",
        "aws-sdk": "^2.716.0",
        "axios": "^0.19.2",
        "babel-loader": "^8.1.0",
        "body-parser": "^1.19.0",
        "bootstrap": "^4.5.0",
        "connect-mongo": "^3.2.0",
        "cookie-parser": "^1.4.5",
        "css-loader": "^3.6.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^6.0.0",
        "font-awesome": "^4.7.0",
        "helmet": "^3.22.1",
        "mongoose": "^5.9.18",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "node-sass": "^4.14.1",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^6.0.1",
        "postcss-loader": "^3.0.0",
        "pug": "^3.0.0",
        "sass-loader": "^8.0.2",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    },
    "scripts": {
        "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' --ignore 'src/assets'",
        "dev:assets": "WEBPACK_ENV=development webpack -w",
        "build:assets": "WEBPACK_ENV=production webpack",
        "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','webpack.config.js'",
        "copyAll": "cp -R src/static build && cp -R src/views build",
        "build": "npm run build:server && npm run build:assets && npm run copyAll",
        "prebuild": "rm -rf build",
        "tunnel": "lt --port 4000",
        "start": "PRODUCTION=true node build/init.js",
        "prestart": "npm run build"
    }
}
